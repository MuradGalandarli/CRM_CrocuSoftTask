            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "isActive": {
            "type": "boolean"
          },
          "teamMembers": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "member": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Member"
            },
            "nullable": true
          },
          "projects": {
            "type": "array",
